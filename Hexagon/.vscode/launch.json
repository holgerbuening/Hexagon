{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Qt Project",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "${workspaceFolder}/build",
      "program": "${workspaceFolder}/build/Hexagon1",
      "environment": [],
      "MIMode": "gdb",
      "miDebuggerPath": "/usr/bin/gdb",
      "setupCommands": [
        {
          "description": "Disable pretty-printing for gdb",
          "text": "set print pretty off",
          "ignoreFailures": true
        }],
        "sourceFileMap": {
          "/usr/include/qt": "${workspaceFolder}/home/holger/Qt/include",
          "/usr/include/QtCore": "${workspaceFolder}/home/holger/Qt/include/QtCore",
          "/usr/include/QtGui": "${workspaceFolder}/home/holger/Qt/include/QtGui",
          "/usr/include/QtWidgets": "${workspaceFolder}/home/holger/Qt/include/QtWidgets"
        },
      "preLaunchTask": "build" 
      
    }
  ]
}